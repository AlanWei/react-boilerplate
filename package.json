{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React 16.0 boilerplate",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress --colors",
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p --progress --colors",
    "clean": "rm -rf build/",
    "lint": "eslint src",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage"
  },
  "author": "Alan Wei",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "connected-react-router": "^4.4.1",
    "history": "^4.6.3",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "moduleNameMapper": {},
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "resetMocks": true,
    "setupFiles": [],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testURL": "http://localhost"
  }
}
