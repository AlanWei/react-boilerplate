{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React 16.0 boilerplate",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env && NODE_ENV=development webpack-dev-server --inline --progress --colors",
    "dev:server": "mock-json-server data.json",
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p --progress --colors",
    "clean": "rm -rf build/",
    "lint": "eslint src/"
  },
  "author": "Alan Wei",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.11.3",
    "mock-json-server": "^1.3.6",
    "node-sass": "^4.14.1",
    "postcss": "^8.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.3",
    "prettier": "^2.1.2",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.3.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.20.0",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "moduleNameMapper": {},
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "resetMocks": true,
    "setupFiles": [],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testURL": "http://localhost"
  }
}
