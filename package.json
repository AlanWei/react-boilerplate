{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React 16.0 boilerplate",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress --colors",
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p --progress --colors",
    "clean": "rm -rf build/",
    "lint": "eslint src",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage"
  },
  "author": "Alan Wei",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "8.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.18.0",
    "connected-react-router": "^6.4.0",
    "history": "^5.0.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "moduleNameMapper": {},
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "resetMocks": true,
    "setupFiles": [],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testURL": "http://localhost"
  }
}
